{
  "name": "@js-telecortex-2/js-telecortex-2-server",
  "version": "0.1.2-alpha",
  "description": "A NodeJS server for controlling APA102/SK9822 LED strips over OPC",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch --coverage",
    "test-ci": "jest --ci --coverage",
    "coverage": "codecov",
    "build": "webpack",
    "dev": "nodemon --exec yarn run babel-node -- src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laserphile/JS-Telecortex-2-Server.git"
  },
  "author": "moonbase-labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Laserphile/JS-Telecortex-2-Server/issues"
  },
  "homepage": "https://github.com/Laserphile/JS-Telecortex-2-Server#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.5.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "node-loader": "^0.6.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@js-telecortex-2/js-telecortex-2-util": "1.1.6",
    "binary": "^0.3.0",
    "chalk": "^2.4.2",
    "dgram": "^1.0.1",
    "lodash": "^4.17.11",
    "pi-spi": "^1.0.3",
    "rpi-ws281x-native": "^0.9.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/testing/setupTests.js"
    ]
  }
